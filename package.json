{
  "name": "statsim",
  "version": "0.11.0",
  "description": "Web app for statistical simulations and bayesian inference",
  "main": "electron.js",
  "private": true,
  "scripts": {
    "build": "npm run build-web && npm run build-bin",
    "build-web": "browserify src/main.js -t [ vueify ] -t [ babelify ] -g [ envify --NODE_ENV production ] | uglifyjs -cm > dist/bundle.js",
    "build-bin": "rm -r bin && mkdir bin && electron-packager . --ignore=\"(node_modules|src)\"  --platform linux,win32 --out bin && cd ./bin && for i in */; do zip -r \"${i%/}.zip\" \"$i\"; done",
    "build-dev": "browserify src/main.js -t [ vueify ] -o dist/bundle.js --debug",
    "watch": "nodemon --watch src --ext vue,js,css,html --exec 'npm run build-dev'",
    "electron": "electron .",
    "test-server": "http-server -s -p 8085 & tape test.js | faucet && kill $!",
    "test": "tape test.js | faucet"
  },
  "author": "Anton Zemlyansky",
  "license": "ISC",
  "dependencies": {
    "cookie": "^0.3.1",
    "csv-parse": "^3.2.0",
    "d3-array": "^1.2.4",
    "d3-hist2d": "^1.0.7",
    "density-plot": "^1.0.4",
    "dygraphs": "^2.1.0",
    "file-saver": "^2.0.0",
    "handsontable": "^6.2.0",
    "html-escape": "^2.0.0",
    "is": "^3.2.1",
    "is-json": "^2.0.1",
    "js-beautify": "^1.8.9",
    "js-quantities": "^1.7.2",
    "online-stats": "^1.3.0",
    "pdfast": "^0.2.0",
    "query-string": "^6.2.0",
    "vue": "^2.5.17",
    "vue-color": "^2.7.0",
    "vue-material": "^0.8.1",
    "vue2vis": "0.0.15",
    "vuedraggable": "^2.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "electron": "^3.0.10",
    "electron-packager": "^12.2.0",
    "envify": "^4.1.0",
    "faucet": "0.0.1",
    "http-server": "^0.11.1",
    "nodemon": "^1.18.7",
    "puppeteer": "^1.12.2",
    "tape": "^4.9.1",
    "uglify-es": "^3.3.10",
    "vueify": "^9.4.1"
  }
}
